<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE en-note SYSTEM "http://xml.evernote.com/pub/enml2.dtd">
<en-note style="font-weight: normal; font-size: 12px;" xmlns:h="http://www.r00tsh3ll.org/">
  <div>
    <div style="margin-bottom: 5px;">
          <en-media
                  h:hash="faviconHash"
                  h:type="faviconMime"
                  style="margin-right: 5px; vertical-align: middle;"/>
          <a h:href="url" style="white-space: nowrap; text-decoration:none;"><h:txt id="url"/></a>
    </div>

    <en-media
     h:hash="hash"
     h:type="mime"
     style="vertical-align: top;
     display: inline-block;
     margin-top: 10px;
     margin-right: 10px;
     max-width: 50%;"/>
    <div
     style="display: inline-block;
     vertical-align: top;
     margin-top: 10px;
     max-width: 50%;">
        <h:txt id="snippet"/>
    </div>
  </div>
</en-note>
